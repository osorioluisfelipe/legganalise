<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-md-8">
      <h1>Add new sample_analysis <%= @sample.sample_name %></h1>

      <%= simple_form_for [@sample, @sample_analysis] do |f| %>
        <%= f.input :lab_analysis, collection: LabAnalysis.all, input_html: { multiple: true, class: "multiple-select" }, include_hidden: false %>
        <%= f.submit "Add tag", class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
