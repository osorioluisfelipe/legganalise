<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-md-8">
      <h1>Add new sample_analysis <%= @sample.sample_name %></h1>

      <%= simple_form_for [@sample, @sample_analysis] do |f| %>
        <%#= raise %>
        <%= f.collection_check_boxes :lab_analysis_id, LabAnalysis.all, :id, :analysis_name do |m| %>
          <%= m.check_box %> <%= m.label %>
        <% end %>
        <%= f.submit "Add tag", class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
